    <div>
        @foreach (System.Data.DataRow dr in ViewBag.IE.Rows)
        {

            using (Html.BeginForm("UpdateIE", "IE", FormMethod.Post, new { @Id = "formid" }))
            {
                @Html.AntiForgeryToken()

                <div class="form-horizontal">
                    <h4>IE</h4>
                    <hr />
                    @Html.ValidationSummary(false, "", new { @class = "text-danger" })

                    <div class="form-group">
                        @Html.LabelFor(model => model.Question, htmlAttributes: new { @class = "control-label col-md-2" })
                        <div class="col-md-10">
                            <textarea type="text"  name="Question">@dr.ItemArray[1]</textarea>
                            @*<input type="text" value="@dr["Question"]" namse="Question" />*@
                        </div>
                    </div>
                    @Html.HiddenFor(m => m.Id)
                    <div class="form-group">
                        @Html.LabelFor(model => model.Answer, htmlAttributes: new { @class = "control-label col-md-2" })
                        <div class="col-md-10">
                            <textarea type="text"  name="Answer">@dr.ItemArray[2]</textarea>
                           @* <input type="text" value="@dr["Answer"]" name="Answer" />*@
                        </div>
                    </div>
                    <div>
                        <input type="submit" value="Update" />
                    </div>

                </div>
            }

        }

    </div>

