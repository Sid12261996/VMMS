        function LoadTransaction(startDate, EndDate, CustName, City, State, Status) {
            debugger;
            $.ajax({
                type: "POST",

                url: "/VMMAssign/ViewDashBoardReport",

                data: { StartDate: startDate, EndDate: EndDate, CustName: CustName, City: City, State: State, Status: Status },
                async: false,
                dataType: "json",
                success: function (data) {
                    debugger;
                    $("#PnlShowLeaveList").empty();
                    var table = $('#tblAttendance').DataTable();
                    table.destroy();
                    var tempTable;
                    tempTable = "<table id='tblAttendance' width='100%' Class='table pmd-table'><thead><tr>"
                    tempTable += "<th> Ticket Id</th > <th>Assets No</th> <th>Assets Type </th>"
                    if (@usertype== 1)
                    {
                        tempTable += "<th> Customer </th>"
                    }
                    tempTable += "<th> Vendor </th>"
                    tempTable += "<th>State</th > <th>City</th>"
                    tempTable += "<th> Status</th >"
                    tempTable += "<th> Host Name</th> <th>Check Antivirus Defination Version</th> <th>Check Windows Update Version</th> <th>Check C Drive Available Space</th> <th>Execute Temp.Files Deletation</th> <th>Execute Pre-Fatch Cleaning</th> <th>Execute Cookies Cleaning</th> <th>Check Earthing Voltage in Power Socket</th> <th>Clean Battery Terminal</th> <th>Check Cabinet Status ( Open/Close )</th> <th>Check Cables Redressing</th> <th>Check UPS Backup</th> <th>Check I/O port ( Damaged /Ok )</th> <th>Check Dusty Atmosphere/Machine ( Internal & External )</th> <th>Check Server Room Temperature</th> <th>Check HDD Bad Sectors</th> <th>Clean CPU Fan Dust</th> <th>Clean Machine Fan Dust</th> <th>Physical Assets Verification Activities</th> <th>Select Asset Type</th> <th>Select Asset Make</th> <th>Capture Asset Model no.</th> <th>Capture Asset product no.</th> <th>Capture Asset Serial no.</th> <th>Capture User Name</th> <th>Capture Employee Code</th> <th>Capture Department Name</th> <th>Capture User  Contact No.</th> <th>Select Asset Status</th> <th>Capture Operating System Name</th> <th>Capture RAM Capacity</th> <th>Capture RAM Quantity</th> <th>Capture HDD Capacity</th> <th>Capture HDD Quantity</th> <th>Capture processor Capacity</th> <th>Capture Processor Quantity</th> <th>Capture Host Name</th> <th>Capture IP Address</th> <th>Capture State</th> <th>Capture City</th> <th>Capture Region</th> <th>Capture Address</th> <th>Capture MS office version</th> <th>Select Availability of Asset</th> <th>Capture user Email ID</th> <th>Capture Asset Branch Code</th>"
                    tempTable += "</tr ></thead > ";
                    // CODE FROM //
                    var Obj = ProccessingForOutPut(data);
                    Object.keys(Obj).forEach(function (item) {
                    Object.keys(Obj[item].Assets).forEach((key) => {

                            tempTable += "<tr>";
                            tempTable += `<td style='width:250px'> ${Obj[item].TransactionNo} <input id='HiddenVendorId' type='hidden' name='HiddenVendorId'  value=" + item.TransactionNo + " /></td>
                                <td style='width:250px'>${ key} </td>
                                <td style='width:250px'>${ Obj[item].AssetsType} </td>`
                                 if (@usertype== 1) {
                                  tempTable += `<td style='width:250px'>${Obj[item].CustName}</td>`
                                  }
                               
                               tempTable += `<td style='width:250px'>${Obj[item].Name} </td>
                                <td style='width:250px'>${Obj[item].State} </td>
                                <td style='width:250px'>${Obj[item].City} </td>
                                <td style='width:250px'>${Obj[item].DND} </td>`;
                              
                            // <td style='width:250px'></td>`;
                            //tempTable += "<td style='width:250px'>" + item.Description + "</td>";
                            console.log(Obj[item].TransactionNo)
                            var description = Obj[item].Assets[key];
                            var num = [];
                            description.forEach((t) => {
                                num.push(t.split('*#*')[1]);

                            });
                            num.sort(function (a, b) { return a - b });
                            description.forEach((ele, index) => {


                                for (var i = 1; i < (num[index + 1] - num[index]); i++) {
                                    tempTable += "<td style='width:250px'></td>"
                                }
                                tempTable += "<td style='width:250px'><a href='https://www.w3schools.com/html/'>" + ele.split('*#*')[0] + "</a></td>";
                            });

                            tempTable += "</tr>";
                            tempTable += "";

                        });


                    });



                    // END
                    tempTable += "</tbody>";
                    tempTable += "</table>";
                    $("#PnlShowLeaveList").append(tempTable);


                    $('#tblAttendance').DataTable({
                        "pageLength": 5,
                        dom: 'Bfrtip',
                        buttons: [
                            'csv', 'excel', 'pdf', 'print'
                        ]
                    });



                }




            });




        }


    </script>

